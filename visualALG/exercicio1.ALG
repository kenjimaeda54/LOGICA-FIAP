Algoritmo "exercio1"

Var
numeroUm,numeroDois,numeroTres: inteiro
resultado: inteiro

Inicio
escreva("Entre com um numero: ")
leia(numeroUm)
escreva("Por favor digite outro numero: ")
leia(numeroDois)
escreva("Digite o ultimo numero inteiro: ")
leia(numeroTres)

resultado := numeroUm+numeroDois+numeroTres

escreva("Soma dos tres numeros é",resultado)


Fimalgoritmo
